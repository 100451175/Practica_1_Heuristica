/*Sección de datos*/
data;

/*Sets*/

#Conjunto de paradas
set Paradas := o s1 s2 s3 d; 

/*Parámetros*/

/*Matriz con los costes de cada arco*/
param distancia :   o  s1  s2  s3   d:=
                o  99   8  10  10  99 
                s1  8  99   3   7   6
                s2 10   3  99   5   7
                s3 10   7   5  99   4
                d  99   6   7   4  99;

/*Vector con la cantidad de gente esperando en cada parada*/
param gente :=
            o   0
            s1  15
            s2  5
            s3  10
            d   0;

#Escalar que contiene el precio de cada autobus
param precio_por_autobus  := 120; 

#Escalar que contiene el precio a pagar por kilometro recogido
param precio_por_km := 5; 

#Escalar que contiene la capacidad máxima de los autobuses
param capacidad_autobus := 20; 

#Escalar que contiene el número de autobuses disponibles
param n_autobuses := 3; 

end;